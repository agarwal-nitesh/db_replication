#!/Users/niteshagarwal/code/db_replication/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'db-replication','console_scripts','mysql-replicate'
__requires__ = 'db-replication'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('db-replication', 'console_scripts', 'mysql-replicate')()
    )
